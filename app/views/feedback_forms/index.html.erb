<p style="color: green"><%= notice %></p>

<% content_for :title, "Feedback forms" %>

<h1>Feedback forms</h1>

<div id="feedback_forms">
  <% @feedback_forms.each do |feedback_form| %>
    <%= render feedback_form %>
    <p>
      <%= link_to "Show this feedback form", feedback_form %>
    </p>
    <div style="border-top: 1px solid #ccc; margin: 15px 0;"></div>
  <% end %>
</div>

<%= link_to "New feedback form", new_feedback_form_path %>
